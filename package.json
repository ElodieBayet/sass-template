{
  "name": "sass-template",
  "version": "2.0",
  "description": "Example of a Front-End template with Sass",
  "main": "index.html",
  "scripts": {
    "dist": "npm run dist:index && npm run dist:unavailable && npm run dist:structure && npm run dist:theme && npm run dist:form",
    "dist:index": "sass --no-source-map ./sass/index.scss:./dist/index.min.css --style compressed",
    "dist:unavailable": "sass --no-source-map ./sass/unavailable.scss:./dist/unavailable.min.css --style compressed",
    "dist:structure": "sass --no-source-map ./sass/structure.scss:./dist/structure.min.css --style compressed",
    "dist:theme": "sass --no-source-map ./sass/theme.scss:./dist/theme.min.css --style compressed",
    "dist:form": "sass --no-source-map ./sass/form.scss:./dist/form.min.css --style compressed"
  },
  "keywords": [
    "Sass",
    "CSS",
    "HTML",
    "JavaScript"
  ],
  "author": "Elodie Bayet",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {}
}